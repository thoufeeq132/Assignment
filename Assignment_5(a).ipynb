    "class Point:\n",
    "    \n",
    "    def __init__(self,x,y,z):\n",
    "        self.x=x\n",
    "        self.y=y\n",
    "        self.z=z\n",
    "        \n",
    "    def sqSum(self):\n",
    "        return self.x**2+self.y**2+self.z**2\n",
    "    \n",
    "obj=Point(1,3,5)\n",
    "sum_of_squares=obj.sqSum()\n",
    "print(sum_of_squares)\n"
   ]
