   "source": [
    "list1 = [8, 2, 3, 0, 7]\n",
    " \n",
    "def sum_of_List(list, size):\n",
    "    if (size == 0):\n",
    "        return 0\n",
    "    else:\n",
    "        return list[size - 1] + sum_of_List(list, size - 1)\n",
    "    \n",
    "total = sum_of_List(list1, len(list1))\n",
    " \n",
    "print(\"Sum of all elements in given list: \", total)"
   ]
